@{
    ViewBag.Title = "试卷";
    Layout = "~/Views/Shared/_PageEdit.cshtml";
}
<div id="QuestionContainer" class="swiper-container">
    <div class="swiper-wrapper">
    </div>
    <div class="swiper-scrollbar">
    </div>
</div>
<link type="text/css" rel="Stylesheet" href="@Url.Content("/Content/lib/swiper_3.1.0/css/swiper.css")" />
<style>
    body
    {
        background: #eee;
        font-family: Helvetica Neue, Helvetica, Arial, sans-serif;
        font-size: 14px;
        color: #000;
        margin: 0;
        padding: 0;
    }
    .swiper-container
    {
        width: 250px;
        height: 100%;
        margin: 0;
    }
    .swiper-slide
    {
        text-align: center;
        font-size: 18px;
        background: #fff;
        width: 250px; /* Center slide text vertically */
        height: 32px;
        display: -webkit-box;
        display: -ms-flexbox;
        display: -webkit-flex;
        display: flex;
        -webkit-box-pack: center;
        -ms-flex-pack: center;
        -webkit-justify-content: center;
        justify-content: center;
        -webkit-box-align: center;
        -ms-flex-align: center;
        -webkit-align-items: center;
        align-items: center;
    }
</style>
<script type="text/javascript" src="@Url.Content("/Content/lib/jquery-tmpl/jquery.tmpl.min.js")"></script>
<script type="text/javascript" src="@Url.Content("/Content/lib/swiper_3.1.0/js/swiper.min.js")"></script>
<script type="text/javascript" src="@Url.Content("/Content/lib/js.request.js")"></script>
<script type="text/javascript" src="@Url.Content("/Content/js/ExaminationPaper/Paper.js")"></script>
<script id="QuestionTmpl" type="text/x-jquery-tmpl">
    <div class="swiper-slide">${type}</div>
</script>
