@{
    ViewBag.Title = "考试任务";
    Layout = "~/Views/Shared/_Page.cshtml";
}
<nav class="breadcrumb"><i class="Hui-iconfont">&#xe67f;</i> 系统 <span class="c-gray en">&gt;</span> 考试任务 <a class="btn btn-success radius r mr-20" style="line-height:1.6em;margin-top:3px" href="javascript:location.replace(location.href);" title="刷新" ><i class="Hui-iconfont">&#xe68f;</i></a></nav>
<div class="list-body pd-20">
    <div id="ExaminationTaskTab" class="HuiTab mt-10 pl-5 pr-5">
      <div class="tabBar cl">
        <span class="tab-btn mr-5" data-value="-1">全部</span>
        <span class="tab-btn mr-5" data-value="0">手动任务</span>
        <span class="tab-btn mr-5" data-value="1">自动任务</span>
        <span class="tab-btn" data-value="2">预定任务</span>
      </div>
      <div class="tabCon">
        <div class="mt-20">
            <table class="table table-border table-bordered table-bg table-hover table-sort">
                <thead>
                    <tr>
                        <th class="text-c">
                            <input type="checkbox" name="" value="" />
                        </th>
                        <th>
                            ID
                        </th>
                        <th>
                            名称
                        </th>
                        <th>
                            任务类型
                        </th>
                        <th>
                            任务时间
                        </th>
                        <th>
                            时长
                        </th>
                        <th>
                            自动类型
                        </th>
                        <th>
                            备注
                        </th>
                        <th class="text-c">
                            操作
                        </th>
                    </tr>
                </thead>
                <tbody>
                </tbody>
            </table>
        </div>
      </div>
      <div class="tabCon"></div>
      <div class="tabCon"></div>
      <div class="tabCon"></div>
    </div>
    <div class="cl"></div>
</div>
<script type="text/javascript" src="@Url.Content("~/Contents/lib/js.datetime.js")"></script>
<script type="text/javascript" src="@Url.Content("~/Contents/lib/My97DatePicker/WdatePicker.js")"></script>
